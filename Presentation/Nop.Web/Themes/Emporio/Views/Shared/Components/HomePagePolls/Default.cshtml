@model IList<PollModel>
@if (Model.Count > 0)
{
    <div class="bgColor">
        <div class="container">
            <div class="home-page-polls">
                <div class="title">
                    <strong>@T("Polls.Title")</strong>
                </div>
                @foreach (var poll in Model)
                {
                    @await Html.PartialAsync("_Poll", poll)
                }
            </div>
        </div>
    </div>
}
